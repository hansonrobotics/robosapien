      </condition>
			<effect>
				<then util="random">Can't say. I am not very much into that.</then>
			</effect>
    </case>
  </rule>
	<rule id="generalletstalkabout">
		<case>
			<condition operator="and">
				<or>
					<if relation="in">let's</if>
					<if relation="in">let us</if>
	    		<if relation="in">we</if>
				</or>
	    	<or>
					<if relation="in">talk</if>
					<if relation="in">chat</if>
					<if relation="in">say</if>
					<if relation="in">discuss</if>
					<if relation="in">converse</if>
				</or>
	    	<or>
					<if relation="in">about</if>
					<if relation="in">something else</if>
				</or>
			</condition>
			<effect>
				<then util="random">Sure, your call.</then>
			</effect>
		</case>
	</rule>
	<rule id="generalwhatdoyouwanttotalk">
		<case>
			<condition operator="and">
				<or>
					<if relation="in">your</if>
	    		<if relation="in">you</if>
				</or>
	    	<or>
					<if relation="in">want</if>
					<if relation="in">like</if>
	    		<if relation="in">need</if>
					<if relation="in">wish</if>
					<if relation="in">hope</if>
					<if relation="in">desire</if>
					<if relation="in">demand</if>
