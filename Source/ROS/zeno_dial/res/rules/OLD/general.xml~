					<if relation="in">crave</if>
				</or>
	    	<or>
					<if relation="in">to talk</if>
					<if relation="in">to chat</if>
					<if relation="in">to say</if>
					<if relation="in">to discuss</if>
					<if relation="in">to converse</if>
				</or>
			</condition>
			<effect>
				<then util="random">Your call, surprise me.</then>
				<then util="random">Let's talk about music. Do you listen to music?</then>
				<then util="random">Let's talk about dream. Do you dream a lot?</then>
			</effect>
		</case>
	</rule>
