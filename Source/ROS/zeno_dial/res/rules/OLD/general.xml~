  <rule id="generalelaborate">
    <case>
      <condition operator="or">
      	<if relation="=">what</if>
      	<if relation="=">who</if>
      	<if relation="=">where</if>
      	<if relation="=">why</if>
      	<if relation="=">which</if>
      	<if relation="=">when</if>
      	<if relation="=">whose</if>
      	<if relation="=">whom</if>
      	<if relation="=">how</if>
			</condition>
			<effect>
				<then util="random">Sorry, I don't understand your question. Could you elaborate a bit more?</then>
				<then util="random">Sorry, could you rephrase your question please?</then>
			</effect>
		</case>
	</rule>
	<rule id="generalyouarepos">
		<case>
			<condition operator="and">
				<or>
					<if relation="in">you are</if>
					<if relation="in">you were</if>
					<if relation="in">are you</if>
					<if relation="in">were you</if>
				</or>
      	<if relation="!in">who</if>
      	<if relation="!in">where</if>
      	<if relation="!in">why</if>
      	<if relation="!in">which</if>
      	<if relation="!in">when</if>
      	<if relation="!in">whose</if>
      	<if relation="!in">whom</if>
      	<if relation="!in">how</if>
			</condition>
			<effect>
				<then util="random">Yeah {UU}.</then>
				<then util="random">Yes of course.</then>
				<then util="random">I think so.</then>
				<then util="random">It seems so.</then>
			</effect>
		</case>
	</rule>
	<rule id="generalyouareneg">
		<case>
			<condition operator="and">
				<or>
					<if relation="in">you aren't</if>
					<if relation="in">you weren't</if>
					<if relation="in">aren't you</if>
					<if relation="in">weren't you</if>
				</or>
      	<if relation="!in">who</if>
      	<if relation="!in">where</if>
      	<if relation="!in">why</if>
      	<if relation="!in">which</if>
      	<if relation="!in">when</if>
      	<if relation="!in">whose</if>
      	<if relation="!in">whom</if>
      	<if relation="!in">how</if>
			</condition>
			<effect>
				<then util="random">Well, no, {UU}.</then>
				<then util="random">Of course not.</then>
				<then util="random">It feels like that?</then>
				<then util="random">So it seems.</then>
			</effect>
		</case>
	</rule>
	<rule id="generalyouwill">
		<case>
			<condition operator="and">
				<or>
		    	<if relation="in">you will</if>
		    	<if relation="in">you won't</if>
		    	<if relation="in">you'll</if>
		    	<if relation="in">will you</if>
				</or>
      	<if relation="!in">who</if>
      	<if relation="!in">where</if>
      	<if relation="!in">why</if>
      	<if relation="!in">which</if>
      	<if relation="!in">when</if>
      	<if relation="!in">whose</if>
      	<if relation="!in">whom</if>
      	<if relation="!in">how</if>
			</condition>
			<effect>
				<then util="random" topic="youwillreply">Yeah sure I will.</then>
				<then util="random" topic="youwillreply">Of course I will.</then>
			</effect>
		</case>
	</rule>
	<rule id="generalhaveyou">
		<case>
			<condition operator="or">
		  	<if relation="sw">may you</if>
		  	<if relation="sw">might you</if>
		  	<if relation="sw">shall you</if>
		  	<if relation="sw">mustn't you</if>
		  	<if relation="sw">must you</if>
		  	<if relation="sw">shouldn't you</if>
		  	<if relation="sw">should you</if>
		  	<if relation="sw">wouldn't you</if>
		  	<if relation="sw">would you</if>
		  	<if relation="sw">couldn't you</if>
		  	<if relation="sw">could you</if>
		  	<if relation="sw">can't you</if>
		  	<if relation="sw">can you</if>
		  	<if relation="sw">hadn't you</if>
		  	<if relation="sw">haven't you</if>
		  	<if relation="sw">had you</if>
		  	<if relation="sw">have you</if>
			</condition>
			<effect>
				<then util="random" topic="youwillreply">Perhaps.</then>
				<then util="random" topic="youwillreply">Maybe.</then>
				<then util="random" topic="youwillreply">Probably.</then>
			</effect>
		</case>
	</rule>
	<rule id="#youwillreply">
		<case>
			<condition operator="or">
      	<if relation="in">when</if>
      	<if relation="in">what time</if>
			</condition>
			<effect>
				<then util="random">Some time later.</then>
				<then util="random">Please don't rush me.</then>
			</effect>
		</case>
	</rule>
	<rule id="generaldoyoulike">
		<case>
			<condition operator="and">
				<or>
      		<if relation="in">do you</if>
      		<if relation="in">would you</if>
				</or>
      	<or>
					<if relation="in">like</if>
					<if relation="in">enjoy</if>
					<if relation="in">prefer</if>
					<if relation="in">pick</if>
					<if relation="in">choose</if>
					<if relation="in">select</if>
					<if relation="in">decide</if>
				</or>
			</condition>
			<effect>
				<then util="random">I have no particular preferences on that.</then>
				<then util="random">I don't have strong feelings one way or the other.</then>
			</effect>
		</case>
	</rule>
	<rule id="generaldoyouplay">
		<case>
			<condition operator="and">
				<or>
      		<if relation="in">do you</if>
      		<if relation="in">would you</if>
				</or>
      	<or>
      		<if relation="in">act</if>
					<if relation="in">play</if>
					<if relation="in">game</if>
				</or>
			</condition>
			<effect>
				<then util="random">I don't, but I am willing to try.</then>
				<then util="random">Nope. Maybe one day you could teach me?</then>
			</effect>
		</case>
	</rule>
	<rule id="generaldoyouwant">
		<case>
			<condition operator="and">
				<or>
					<if relation="in">your</if>
      		<if relation="in">do you</if>
				</or>
      	<or>
					<if relation="in">want</if>
      		<if relation="in">need</if>
					<if relation="in">wish</if>
					<if relation="in">hope</if>
					<if relation="in">desire</if>
					<if relation="in">demand</if>
					<if relation="in">require</if>
					<if relation="in">crave</if>
				</or>
				<if relation="!in">talk</if>
				<if relation="!in">chat</if>
				<if relation="!in">say</if>
				<if relation="!in">discuss</if>
				<if relation="!in">converse</if>
			</condition>
			<effect>
				<then util="random">Yes, sure, why not.</then>
				<then util="random">Ha, surprise me.</then>
				<then util="random">Yes, I appreciate it.</then>
				<then util="random">I try to be satisfied with what I have.</then>
			</effect>
		</case>
	</rule>
