	<rule id="generalrepeatlast" cache="no">
		<case>
			<condition operator="or">
				<if relation="in">say it again</if>
				<if relation="in">what * was that again</if>
				<if relation="in">repeat</if>
				<if relation="in">what * did you just say</if>
				<if relation="in">once more</if>
				<if relation="in">I can't hear you</if>
				<if relation="in">I cannot hear you</if>
				<if relation="in">I couldn't hear you</if>
				<if relation="in">I could not hear you</if>
				<if relation="in">louder please</if>
				<if relation="in">don't understand * you</if>
				<if relation="in">didn't understand * you</if>
				<if relation="in">excuse me</if>
        <and>
          <or>
				    <if relation="in">what * you mean</if>
				    <if relation="in">what * you meant</if>
          </or>
			    <if relation="!in">is * what</if>
			    <if relation="!in">was * what</if>
			    <if relation="!in">are * what</if>
			    <if relation="!in">were * what</if>
        </and>
			</condition>
			<effect>
        <then topic="@repeat" />
			</effect>
		</case>
	</rule>
</category>
